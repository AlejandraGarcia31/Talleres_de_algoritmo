Algoritmo Ejercicio_dieciocho
	//entradas
	Escribir "Digite su primer nombre: "
	Leer nombre
	Escribir  "Digite su primer apellido: "
	Leer apellido
	Escribir "Digite su segundo apellido: "
	Leer apellido_dos
	//caja negra
	N<-SubCadena(nombre,1,1)
	a<-SubCadena(apellido,1,1)
	ap<-SubCadena(apellido_dos,1,1)
	//salida
	Escribir N, a, ap
FinAlgoritmo
